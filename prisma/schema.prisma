generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String // MediumText may be needed for MySql
  expiresAt DateTime
}

model User {
  id       Int    @id @default(autoincrement())
  fullname String
  email    String @unique
  password String
}
